<section class="section volunteer-section">
	<table>
		<thead>
		  <tr>
			
			<th>SNO</th>
			<th>Trust Name </th>
			<th>Ratings </th>
			
		  </tr>
		</thead>
		<tbody>
			<% 
			
			trusts.sort((a, b) => {
				if(typeof ratings[b.name] == 'undefined'){
					ratings[b.name]=0;
				}
				if(ratings[a.name] == 'undefined'){
					ratings[a.name]=0;
				}
			
				return ratings[b.name] - ratings[a.name];
			});
		
			for (let i = 0; i < trusts.length; i++) { %>
				<tr>
					<td><%= i + 1 %></td>
					<td><a href=""><%= trusts[i].name %></a></td>
					<td><% 
						const rating = ratings[trusts[i].name];
						
						let stars = Math.round(rating); // Round the rating to nearest integer
						if(isNaN(stars)){
							stars=0
						}
						if(stars==0){ %>
							<div style="display:inline-block; color: green;">&#9734;</div>
							<div style="display:inline-block; color: green;">&#9734;</div>
							<div style="display:inline-block; color: green;">&#9734;</div>
							<div style="display:inline-block; color: green;">&#9734;</div>
							<div style="display:inline-block; color: green;">&#9734;</div>

						<%  } 
						else {
							for (let j = 0; j < stars; j++) { %>
								<div style="display:inline-block; color: green;">&#9733;</div>
							
							<% } %>
							<% for(k=stars;k< 5;k++){ %>
								<div style="display:inline-block; color: green;">&#9734;</div>
							<% } }%>
					
					</td>
				</tr>
			<% } %>
		</tbody>
		
	  </table>

	






</section>




