              >
              <div class="foot_input">
                <input
                  type="text"
                  placeholder="Name"
                  class="news_letter"
                  name="name"
                  required
                />
              </div>
              <div class="foot_input subs_btn">
                <input
                  type="email"
                  name="news_email"
                  id="news_letter"
                  class="news_letter"
                  placeholder="Email id"
                  required
                /><button type="submit" class="sign-in-btn">Subscribe</button>
              </div>
