    <div class="display-reviews">
        <h1 class="review-heading">reviews</h1>
        <% jone.forEach(element=> { %>
            <div class="review-box">
                <div class="review-p-name">Name:  <%- element.review_name %></div>

                <div class="review-p">Rating: 
                    <% 

                        const rating = element.review_rating;
                        const filledStars = Math.floor(rating);
                        const emptyStars = 5 - filledStars; 
                    %>
                    <% for (let i = 0; i < filledStars; i++) { %>
                        <span class="review-star">&#9733;</span> 
                    <% } %>
                    <% for (let i = 0; i < emptyStars; i++) { %>
                        <span class="star">&#9734;</span> 
                    <% } %>
                </div>
                


                
                <div class="review-p-comment">Review:<br> <%- element.review_comment %></div>
            </div>
            <br>
    
        <% }) %>
